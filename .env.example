# TuneAtLife Portal Environment Variables

# Sentry Configuration
VITE_SENTRY_DSN=https://your-dsn-here@o<org-id>.ingest.sentry.io/<project-id>
VITE_SENTRY_RELEASE=tuneatlife-portal@dev
VITE_ENVIRONMENT=development

# Sentry Build Configuration (for CI/CD)
SENTRY_ORG=tuneatlife
SENTRY_PROJECT=tuneatlife-portal
SENTRY_AUTH_TOKEN=your-auth-token-here

# API Configuration
VITE_API_URL=https://tuneatlife-beta-api.fly.dev
VITE_API_VERSION=v1

# Feature Flags
VITE_DEBUG=false
VITE_ENABLE_ANALYTICS=true